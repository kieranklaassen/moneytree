<p id="notice"><%= notice %></p>

<h1>Marketplace Orders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @marketplace_orders.each do |marketplace_order| %>
      <tr>
        <td><%= link_to 'Show', marketplace_order %></td>
        <td><%= link_to 'Edit', edit_marketplace_order_path(marketplace_order) %></td>
        <td><%= link_to 'Destroy', marketplace_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

